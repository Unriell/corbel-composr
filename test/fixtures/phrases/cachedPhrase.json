{
    "url": "cache/test",
    "version": "2.3.3",
    "get": {
        "code": "res.status(200).send(Date.now())",
        "cache": {
            "type" : "user",
            "duration": "1m"
        },
        "middlewares": [
          "cache"
        ],
        "doc": {
            "description": "Phrase for testing purposes"
        }
    },
    "post": {
        "code": "res.status(200).send('OK')",
        "cache": {
            "invalidate" : [
                "cache/test",
                "user/me"
            ]
        },
        "middlewares": [
          "cache"
        ],
        "doc": {
            "description": "Phrase for testing purposes"
        }
    }
}
