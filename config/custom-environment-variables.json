{
    "serverName" : "SERVER_NAME",
    "bodylimit" : "50mb",
    "port": "PORT",
    
    "rabbitmq": {
        "host": "RABBITMQ_HOST",
        "port": "RABBITMQ_PORT",
        "username": "RABBITMQ_USERNAME",
        "password": "RABBITMQ_PASSWORD",
        "forceconnect":  {
            "__name": "RABBITMQ_FORCE_CONNECT",
            "__format": "boolean"
        }
    },

    "services": {
        "timeout": "SERVICES_TIMEOUT",
        "retries": "SERVICES_RETRIES",
        "time": "SERVICES_TIME"
    },

    "corbel": {
        "credentials": {
            "clientId": "CREDENTIALS_CLIENT_ID",
            "clientSecret": "CREDENTIALS_CLIENT_SECRET",
            "scopes": "CREDENTIALS_SCOPES"
        },
        "options": {
            "urlBase": "URL_BASE"
        }
    },

    "bunyan": {
        "log" : {
            "__name": "BUNYAN_LOG",
            "__format": "boolean"
        },
        "stdout": {
            "__name": "BUNYAN_STDOUT",
            "__format": "boolean"
        },
        "streamServer": {
            "__name": "BUNYAN_STREAM_SERVER",
            "__format": "boolean"
        }
    },

    "composrLog": {
        "accessLog" : {
            "__name": "ACCESS_LOG",
            "__format": "boolean"
        },
        "accessLogFile" : "ACCESS_LOG_FILE",
        "logLevel": "LOG_LEVEL",
        "logFile": "LOG_FILE",
        "syslog" : {
            "__name": "SYSLOG",
            "__format": "boolean"
        }
    },

    "newrelic": {
        "enabled": {
            "__name": "NRACTIVE",
            "__format": "boolean"
        },
        "name": "NRAPPNAME",
        "key": "NRAPIKEY"
    },

    "keymetrics": {
        "__name": "KEYMETRICS",
        "__format": "boolean"
    },
    
    "execution": {
        "vm": {
            "__name": "EXEC_WITH_VM",
            "__format": "boolean"
        },
        "gc":  {
            "__name": "ENFORCE_GC",
            "__format": "boolean"
        },
        "timeout": "TIMEOUT",
        "local": {
            "__name": "LOCAL_MODE",
            "__format": "boolean"
        }
    }
}
